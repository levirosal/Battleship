pipeline {
    agent any

    stages {

        stage ('Compile Stage') {

            steps {
                sh 'javac -d class/ src/com/levi/model/*.java'
                sh 'javac -d class/ src/com/levi/*.java'
            }
        }

        }

        stage ('Verify Stage') {
            steps {
                sh 'ls -lh class/'
            }
        }
    }
}